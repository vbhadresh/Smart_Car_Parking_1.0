<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="388.0" prefWidth="427.0" styleClass="root"
	stylesheets="@Login.css" xmlns="http://javafx.com/javafx/8.0.40"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.iit.controller.Login">
	<children>

		<Label layoutX="100.0" layoutY="40.0" prefHeight="20.0"
			prefWidth="250.0" text="EVENT BOARD " textFill="#19140e">
			<font>
				<Font size="30.0" />
			</font>
		</Label>

		<Label layoutX="110.0" layoutY="95.0" prefHeight="15.0"
			prefWidth="300.0" text="Username" textFill="#262525">
			<font>
				<Font size="15.0" />
			</font>
		</Label>

		<Label layoutX="115.0" layoutY="150.0" prefHeight="15.0"
			prefWidth="300.0" text="Password" textFill="#262525">
			<font>
				<Font size="15.0" />
			</font>
		</Label>

		<TextField fx:id="username" layoutX="110.0" layoutY="117.0"
			promptText="Enter Username" />
		<PasswordField fx:id="password" layoutX="110.0" layoutY="169.0"
			promptText="Enter Password" />
		<Button layoutX="151.0" layoutY="254.0" mnemonicParsing="false"
			onAction="#loginTraverse" text="Login" textFill="#f80808" />
	</children>
</AnchorPane>
